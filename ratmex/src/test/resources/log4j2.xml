<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="Logging Config" status="warn" monitorInterval="10" strict="true">
  <Properties>
    <Property name="ratmex.charset">UTF-8</Property>
    <Property name="ratmex.layoutPattern">
      [%-5level] [%d{yyyy-MM-dd'T'HH:mm:ss.SSSZ}] [%t] [%logger] - %msg%n
    </Property>
  </Properties>
  <Appenders>
    <Appender type="Console" name="stdout" target="SYSTEM_OUT">
      <Layout type="PatternLayout" charset="${ratmex.charset}">
        <Pattern>${ratmex.layoutPattern}</Pattern>
      </Layout>
    </Appender>
  </Appenders>
  <Loggers>
    <Root level="off">
    </Root>
    <Logger name="stincmale.ratmex.ratmex" level="trace">
      <AppenderRef ref="stdout"/>
    </Logger>
  </Loggers>
</Configuration>